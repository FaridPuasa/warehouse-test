<nav aria-label="Page navigation example">
    <% if (pages > 0) { %>
        <ul class="pagination">
            <% if (current == 1) { %>
                <li class="page-item disabled"><a class="page-link">First</a></li>
            <% } else { %>
                <li class="page-item"><a class="page-link" href="/list/<%=product%>/<%=area%>/1/<%=limit%>">First</a></li>
            <% } %>
            <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
            <% if (i !== 1) { %>
                <li class="page-item disabled"><a class="page-link">...</a></li>
            <% } %>
            <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                <% if (i == current) { %>
                    <li class="page-item active"><a class="page-link"><%= i %><span class="sr-only">(current)</span></a></a></li>
                <% } else { %>
                    <li><a class="page-link" href="/list/<%=product%>/<%=area%>/<%= i %>/<%=limit%>"><%= i %><span class="sr-only">(current)</span></a></li>
                <% } %>
                <% if (i == Number(current) + 4 && i < pages) { %>
                    <li class="page-item disabled"><a class="page-link">...</a></li>
                <% } %>
            <% } %>
            <% if (current == pages) { %>
                <li class="disabled"><a class="page-link">Last</a></li>
            <% } else { %>
                <li class="page-item"><a class="page-link"  href="/list/<%=product%>/<%=area%>/<%= pages %>/<%=limit%>">Last</a></li>
            <% } %>
        </ul>
    <% } %>
</nav>