    <script>
      document.getElementById("showTaskStatus").addEventListener("click", showTaskStatus);

      function showTaskStatus() {
        let countPODtasks = document.getElementsByClassName("tn").length;

        for (let i = 0; i < countPODtasks; i++) {
          var itemTN = document.getElementsByClassName("tn")[i].innerText;

          var request = new XMLHttpRequest();
          request.open('POST', 'https://api.tookanapp.com/v2/get_job_details');
          request.setRequestHeader('Content-Type', 'application/json');

          request.onreadystatechange = function() {
            if (this.readyState === 4) {
              console.log('Status:', this.status);
              console.log('Headers:', this.getAllResponseHeaders());
              console.log('Body:', this.responseText);

              responsejd = this.responseText;
              json_responsejd = JSON.parse(responsejd);

              if (json_responsejd.data[0].job_status == 0) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Assigned";
              }

              if (json_responsejd.data[0].job_status == 1) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Started";
              }

              if (json_responsejd.data[0].job_status == 2) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Successful";
              }

              if (json_responsejd.data[0].job_status == 3) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Failed";
              }

              if (json_responsejd.data[0].job_status == 4) {
                document.getElementsByClassName("taskStatus")[i].innerText = "InProgress/Arrived";
              }

              if (json_responsejd.data[0].job_status == 6) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Unassigned";
              }

              if (json_responsejd.data[0].job_status == 7) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Accepted/Acknowledged";
              }

              if (json_responsejd.data[0].job_status == 8) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Decline";
              }

              if (json_responsejd.data[0].job_status == 9) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Cancel";
              }

              if (json_responsejd.data[0].job_status == 10) {
                document.getElementsByClassName("taskStatus")[i].innerText = "Deleted";
              }
            }
          };

          var body = {
            'api_key': '51676580f24b091114132d38111925401ee4c2f328d978375e1f03',
            'job_ids': [
              itemTN
            ],
            'include_task_history': 0
          };
          request.send(JSON.stringify(body));
        }
      }
    </script>

