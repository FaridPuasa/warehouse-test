    <script>
      const myFunction = () => {
        const trs = document.querySelectorAll('#myTable tr:not(.header)')
        const filter = document.querySelector('#myInput').value
        const regex = new RegExp(filter, 'i')
        const isFoundInTds = td => regex.test(td.innerHTML)
        const isFound = childrenArr => childrenArr.some(isFoundInTds)
        const setTrStyleDisplay = ({
          style,
          children
        }) => {
          style.display = isFound([
            ...children // <-- All columns
          ]) ? '' : 'none'
        }
        trs.forEach(setTrStyleDisplay)
      }
    </script>

    <script>
      let countPODexcel = document.getElementsByClassName("pod-body").length;

      for (let i = 0; i < countPODexcel; i++) {
        var inputElement = document.createElement('button');

        inputElement.addEventListener('click', function() {
          saveAs(new Blob([s2ab(XLSX.write((XLSX.utils.table_to_book(document.getElementsByClassName("table")[i], {
            sheet: "POD"
          })), {
            booktype: 'xlsx',
            bookSST: true,
            type: 'binary'
          }))], {
            type: "application/octet-stream"
          }), 'POD.xlsx');
        });

        inputElement.setAttribute('type', 'button');
        inputElement.innerText = "Download POD as Excel";
        inputElement.setAttribute('class', 'button-3 w-button');

        document.getElementsByClassName("pod-body")[i].appendChild(inputElement);
      }

      function s2ab(s) {
        var buf = new ArrayBuffer(s.length);
        var view = new Uint8Array(buf);
        for (var i = 0; i < s.length; i++) view[i] = s.charCodeAt(i) & 0xFF;
        return buf;
      }
    </script>

