            <tbody>
              <%itemList.forEach(inventory=> {%>
              <tr>
                <th scope="row">
                  <%= inventory.trackingNumber %>
                </th>
                <td>
                  <%= inventory.parcelNumber %>
                </td>
                <td><%= inventory.area %></td>
                <td>
                  <%= inventory.name %>
                </td>
                <td>
                  <%= inventory.address %>
                </td>
                <td>
                  <%= inventory.contact %>
                </td>
                <td>
                  <%= inventory.value %>
                </td>
                <td>
                  <%= inventory.status %>
                </td>
                <td class="counts">
                  <%= inventory.count %>
                </td>
                <td>
                <% for(var i = 0 ; i < inventory.history.length; i++) {%>
                    <ul>
                      <li><%= inventory.history[i].statusDetail %> | <%= inventory.history[i].dateUpdated %></li>
                      
                    </ul>
                <% } %>
                </td>
                <td>
                  <%= moment(inventory.entryDate).format('DD-MM-YYYY') %>
                </td>
                <td class="expire">
                  <%= moment(inventory.expireDate).format('DD-MM-YYYY') %>
                </td>
                <td class="days">
                  <%= inventory.age %>
                </td>
              </tr>
              <%})%>
            </tbody>
          </table>
